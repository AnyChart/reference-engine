<div class="method">
  <h3 id="{{name}}">{{name}}</h3>
  {{#is-inherited}}
    Inherited from {{#type-link}}{{inherited-from}}{{/type-link}}
  {{/is-inherited}}
  <div>
    <ul class="overrides">
    {{#overrides}}
      <li><a href="#{{name}}">{{signature}}</a></li>
    {{/overrides}}
    </ul>
    <div class="overrides">
    {{#overrides}}
      <div>
        {{#has-since}}
        <div class="since">Since version {{since}}</div>
        {{/has-since}}
        <p>
          {{&description}}
          {{#has-detailed}}
          <div class="detailed">
            <a href="#{{name}}" class="btn"><i class="icon-info-circled"></i> Detailed description</a>
            <div style="display:none">
              <a class="close">x</a>
              <p>{{&detailed}}</p>
            </div>
          </div>
          {{/has-detailed}}
        </p>
        {{#has-params}}
        <table class="params">
          <thead>
            <tr>
              <th>Params</th>
              <th>Type</th>
              {{#has-params-defaults}}<th>Default</th>{{/has-params-defaults}}
              <th>Description</th>
            </tr>
          </thead>
          <tbody>
            {{#params}}
              <tr>
                <td class="name code">{{name}}</td>
                <td class="type"><i class="types">{{#types}}<i>{{#type-link}}{{.}}{{/type-link}}</i>{{/types}}</i></td>
                {{#has-params-defaults}}<td class="val">{{&default}}</td>{{/has-params-defaults}}
                <td class="description">{{&description}}</td>
              </tr>
            {{/params}}
          </tbody>
        </table>
        {{/has-params}}
        {{#has-returns}}
        <div class="returns">
          {{#returns}}
          <span class="b">Returns:</span> <i class="types">{{#types}}<i>{{#type-link}}{{.}}{{/type-link}}</i>{{/types}}</i> {{&description}}
          {{/returns}}
        </div>
        {{/has-returns}}
        {{>listing-part}}
        {{>samples-part}}
      </div>
    {{/overrides}}
    </div>
  </div>
</div>

<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="./i/favicon.ico" type="image/x-icon">
    <title>API Reference - AnyChart</title>
    {{#debug}}
    <script src="/dev/react/react.js"></script>
    <link rel="stylesheet" href="/dev/css/styles.css?v={{version}}" />
    <link href="/fonts/font-awesome.css" rel="stylesheet">
    <script src="/dev/goog/base.js"></script>
    <script src="/dev/deps.js"></script>
    <script>
      goog.require('app');
    </script>
    {{/debug}}
  </head>
  <body>
    <div id="sidebar">
      <div class="header">
	<img src="/i/small_logo_white.png" />
	<div>
	  <h1>AnyChart</h1>
	  <h6>api reference</h6>
	</div>
      </div>
      <div class="search">
	<i class="fa fa-search"></i>
	<div>
	  <input type="text" placeholder="search for method in the tree " />
	</div>
      </div>
      <ul class="tree">
	{{#namespaces}}
	  <li><a href="/d/{{.}}"><i class="fa fa-chevron-right"></i>{{.}}</a></li>
	{{/namespaces}}
      </ul>
      <div class="footer">
	<a href="https://twitter.com/anychart" target="_blank"><i class="fa fa-twitter-square"></i></a>
	<a href="https://www.facebook.com/AnyCharts" target="_blank"><i class="fa fa-facebook-square"></i></a>
	<p>&copy; 2014 AnyChart.Com All rights reserved.</p>
      </div>
      <div id="resizer"></div>
    </div>
    <div id="main">
      <div class="header">
	<ul>
	  <li><a href="/">Home</a><span>/</span></li>
	  <li>{{main.full-name}}</a></li>
	</ul>
	<div class="right">
	  <a class="version" href="#">Version 7.2.0 <i class="fa fa-caret-down"></i></a>
	  <div class="support-icon"><i class="fa fa-question-circle"></i></div>
	  <div class="meta">
	    <p>Still have questions?</p>
	    <a href="//support.anychart.com">Contact support</a>
	  </div>
	</div>
      </div>
      <div id="content">
	<div class="line-bottom">
	  <h1>{{main.full-name}}</h1>
	  <p>{{main.description}}</p>
	  <h2>Methods</h2>
	  <label class="checkbox"><input type="checkbox"></input>Show inherited</label>
	  <label class="checkbox"><input type="checkbox"></input>Show static methods</label>
	</div>
	<div class="line-bottom">
	  {{#main.methods}}
	    <div class="method">
	      <h3>{{name}}</h3>
	      <div class="description">
		{{description}}
		{{#members}}
		  <h4>params:</h4>
		  <ul class="params">
		  {{#params}}
		    <li>{{name}} <i class="types">{{#type}}<i>{{.}}</i>{{/type}}</i> {{description}}</li>
		  {{/params}}
		  </ul>
		  <h4>return:</h4>
		  <div class="return">
		    <i class="types">{{#returns.type}}<i>{{.}}</i>{{/returns.type}}</i>  {{returns.description}}
		  </div>
		{{/members}}
	      </div>
	    </div>
	  {{/main.methods}}

	  {{main}}
	</div>
      </div>
    </div>
    <div id="locker" style="display:none"></div>
    <script>
      app.init();
    </script>
  </body>
</html>

<!doctype html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" href="/i/anychart.ico" type="image/x-icon">
        
        <meta property="og:title" content="{{title}}" />
        <meta property="og:type" content="website" />
        <meta property="og:url" content="{{url}}" />
        <meta property="og:image" content="https://cdn.anychart.com/images/features-laptop.png" />
        <meta property="og:site_name" content="AnyChart"/>
        <meta property="og:locale" content="en_US" />
        <meta property="og:description" content="AnyChart HTML5 charts for for web and mobile - API reference{% if page-name %} for {{page-name}}{% endif %}" />
        <meta name="keywords" content="{% if page-name %}{{page-name}}, {% endif %}anychart api reference, js charts, javascript charts, html5 charts, ajax charts, plots, line charts, bar charts, pie charts, js maps, javascript gantt charts, js dashboard" />
        <meta name="description" content="AnyChart HTML5 charts for for web and mobile - API reference{% if page-name %} for {{page-name}}{% endif %}" />
        
        <title>{{title}}</title>
        <!--[if IE]>
        <link rel="stylesheet" type="text/css" href="/lib/bootstrap/css/bootstrap.min.css"/>
        <link rel="stylesheet" type="text/css" href="/lib/bootstrap-select.min.css"/>
        <link rel="stylesheet" type="text/css" href="/lib/jquery-custom-content-scroller/jquery.mCustomScrollbar.min.css"/>
        <link href="/css/font-awesome-4.0.3/css/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="/lib/prettiffy/prettify-tomorrow.css"/>
        <link rel="stylesheet" type="text/css" href="/css/style.css"/>
        <![endif]-->
        <script src="/lib/head.load.min.js"></script>
        <script>
            head.load("/lib/jquery-2.1.3.min.js",
                    "/lib/bootstrap/js/bootstrap.min.js",
                    "/lib/jquery-custom-content-scroller/jquery.mCustomScrollbar.min.js",
                    "/lib/bootstrap-select.min.js",
                    "/lib/bootstrap3-typeahead.min.js",
                    "/lib/prettiffy/prettify.js",
                    "/js/reference.min.js");
        </script>
    </head>
    <body>
        <!--[if !IE]> -->
        <link rel="stylesheet" type="text/css" href="/lib/bootstrap/css/bootstrap.min.css"/>
        <link rel="stylesheet" type="text/css" href="/lib/bootstrap-select.min.css"/>
        <link rel="stylesheet" type="text/css" href="/lib/jquery-custom-content-scroller/jquery.mCustomScrollbar.min.css"/>
        <link href="/css/font-awesome-4.0.3/css/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="/lib/prettiffy/prettify-tomorrow.css"/>
        <link rel="stylesheet" type="text/css" href="/css/style.css"/>
        <!-- <![endif]-->
        <div id="search-results-new" style="display:none">
        </div>
        <div id="header">
            <a class="navbar-brand" href="//anychart.com/">
                <img src="/i/AnyChart-light-empty.png" width="300" height="72" alt="AnyChart">
                <div class="chart-row">
                    <span class="chart-col green"></span>
                    <span class="chart-col orange"></span>
                    <span class="chart-col red"></span>
                </div>
            </a>
            <a class="brand hidden-super-small" href="/"> API Reference</a>
            <div class="pull-right helpers">
                <select class="selectpicker versionselect" data-width="130px">
                    {% for v in versions %}
                    <option{% ifequal v version %} selected{% endifequal %} value="{{v}}">Version {{v}}</option>
                    {% endfor %}
                </select>
                <div class="text-muted questions">
                    <a href="//support.anychart.com/" class="text-support"><i class="icon icon-ring icon-data"></i></a>
                    <span class="hidden-super-small">
                        Still have questions?<br/>
                        <a href="http://anychart.com/support/"> Contact support</a></span>
                </div>
            </div>
        </div>
        <div id="menu-bar">
            <a class="switcher"> <i class="fa fa-chevron-left"></i></a>
            <div id="search-form">
                <form class="form-horizontal" role="form">
                    <div class="form-group has-feedback">
                        <div class="col-sm-12">
                            <input type="text" class="form-control" id="search" placeholder="search" autocomplete="off">
                            <div id="search-results" style="background-color: white;"></div>
                            <span class="glyphicon glyphicon-search form-control-feedback"></span>
                        </div>
                    </div>
                </form>
            </div>
            <div id="tree-wrapper">
                <div id="tree">
                    <ul class="menu">
                    </ul>
                </div>
            </div>
            <div id="footer">
                <div id="footer-inner">
                    <a href="https://www.facebook.com/AnyCharts" class="soc-network" target="_blank">
                        <span class="soc-network-icon fb"><i class="icon icon-fb"></i></span>
                    </a>
                    <a href="https://twitter.com/AnyChart" class="soc-network" target="_blank">
                        <span class="soc-network-icon tw"><i class="icon icon-tw"></i></span>
                    </a>
                    <a href="https://www.linkedin.com/company/386660" class="soc-network" target="_blank">
                        <span class="soc-network-icon in"><i class="icon icon-in"></i></span>
                    </a>
                    <p> © 2015 AnyChart.Com All rights reserved.</p>
                </div>
            </div>
            <div id="size-controller"></div>
        </div>
        <ol class="breadcrumb"></ol>
        <a id="top-page-content">
            Top <i class="fa fa-arrow-up"></i></a>

        {% if not is-last %}
        <div id="warning" class="warning-version alert alert-default fade in">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            <i class="fa fa-exclamation-triangle"></i> You are looking old product version {{version}}. See for actual information <a data-last-version="{{last-version}}" href="/{{last-version}}/try/{{page}}">{{last-version}}</a>.
        </div>
        {% endif %}
        <div id="content-wrapper">
            <div id="content">
                <div class="content-container">
                    {{content|safe}}
                </div>
            </div>
            {% if footer %}
            <div class="content-footer"><hr /><i class="fa fa-info"></i> {{footer|safe}}</div>
            {% endif %}
        </div>
    </body>
    <script>
        var menu_data = {{tree|safe}};
        function node_title(el){
            switch (el.kind){
                case "function": return el.name + "()";
                case "method": return el.name + "()";
                case "enum": return "[" + el.name + "]";
                case "typedef": return "{" + el.name + "}";
            }
            return el.name;
        }
        function is_group(el){
            return el.kind == "class" || el.kind == "namespace";
        }
        function make_menu(el){
            if (is_group(el)) {
                return "<li class='pull-down group' x-data-name='" + el["full-name"] + "'><a href='/{{version}}/" + el["full-name"] +
                        "'><i class='fa fa-chevron-right'></i> " + node_title(el) +  "</a><ul style='display:none'>" +
                        el.children.map(make_menu).join("") + "</ul></li>"
            }else{
                return "<li class='item' x-data-name='" +  el["full-name"] + "'><a href='/{{version}}/" + el["full-name"] +"'>" + node_title(el) + "</a></li>";
            }
        }
        document.getElementsByClassName("menu")[0].innerHTML = make_menu(menu_data[0]);
        head.ready(function () {
            api.core.init("{{version}}", "{{page}}");
        });
    </script>

